{%  extends "base.html.twig" %}

{%  block body %}
    <h1>Add a task</h1>

    {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.description) }}
        {{ form_row(form.enddate) }}
            {{ form_row(form.status) }}
        {{ form_row(form.image) }}
    <button type="submit">Add</button>
    {{ form_end(form) }}

{%  endblock %}